<h1>Teachers</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        <% @teachers.each do |teacher| %>
            <tr>
                <td> <%= link_to "#{teacher.name}", teacher_path(teacher) %> </td>
                <td> <%= teacher.email %> </td>
            </tr>
        <% end %>
    </tbody>
</table>